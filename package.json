{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node --experimental-wasm-threads --experimental-wasm-bulk-memory ./bin/www",
    "dev": "nodemon --experimental-wasm-threads --experimental-wasm-bulk-memory ./bin/www",
    "refresh-tmp": "node ./bin/refresh-tmp.js",
    "rm-source": "rm -f ./public/videos/source/*.*",
    "rm-dash": "rm -rf ./public/videos/dash/*.*",
    "rm-hls": "rm -rf ./public/videos/hls/*.*",
    "rm-videos": "npm run rm-source & npm run rm-dash & npm run rm-hls"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.1",
    "async": "^3.2.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "fluent-ffmpeg": "^2.1.2",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "minio": "^7.0.28",
    "moment": "^2.29.3",
    "mongoose": "^6.3.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.16"
  }
}
